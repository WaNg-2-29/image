function mPrepareCourseListFromAjax() {
    var catid = mLatestSemCatID; // XXX: CatID of current year and sem
    var title = '';
    if (mOnCourseCatPage) {
        let params = new URLSearchParams(document.location.search);
        catid = params.get('categoryid');
    }
    let url = "/course/index.php?categoryid=" + catid + "&perpage=all";
    $.get(url, function(data, status) {
        data = new DOMParser().parseFromString(data, "text/html");
        title = $(data).find('div.page-header-headings > h1').text();
        // Subcats
        var a = $(data).find('div.subcategories').find('a');
        for (let i = 0; i < a.length; i++) {
            let u = a[i].href + "&perpage=all";
            $.get(u, function(dt, st) {
                dt = new DOMParser().parseFromString(dt, "text/html");
                var b = $(dt).find('.coursename > a');
                for (let j = 0; j < b.length; j++) {
                    if (b[j].href.includes('view.php?id=')) {
                        let courseid = b[j].href.split('?id=')[1];
                        let coursename = b[j].innerText;
                        mCourseList.push(mCourseToString(courseid, coursename));
                    }
                }
                mBuildCourseList(title);
            });
        }
        // Immediate courses
        var c = $(data).find('.coursename > a');
        for (let k = 0; k < c.length; k++) {
            if (c[k].href.includes('view.php?id=')) {
                let courseid = c[k].href.split('?id=')[1];
                let coursename = c[k].innerText;
                mCourseList.push(mCourseToString(courseid, coursename));
            }
            mBuildCourseList(title);
        }
    });
}

