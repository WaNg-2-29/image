if (!mDisableEnhancement()) {
    document.addEventListener("DOMContentLoaded", function(event) {
        let listgroupitem = document.querySelectorAll('a.list-group-item.active');
        for (let i = 0; i < listgroupitem.length; i++) {
            listgroupitem[i].classList.remove('active');
        }
        let mycourses = document.querySelectorAll('a.list-group-item[data-parent-key="mycourses"]');
        for (let i = 0; i < mycourses.length; i++) {
            mycourses[i].style.display = "none";
        }
        try {
            document.querySelector('div.list-group-item[data-key="mycourses"]').style.display = "none";
            document.querySelector('a.list-group-item[data-key="home"]').querySelector('span.media-body').innerHTML = "<font color='#014898'><b>" + document.querySelector('a.list-group-item[data-key="home"]').querySelector('span.media-body').innerText + "</b></font>";
            document.querySelector('.categorypicker').remove();
        } catch (e) {
            console.log(e);
        }
        if (window.location.href.includes('/course/index.php')) {
            // try {
            //     document.querySelector('div#page-content').style.display = "none";
