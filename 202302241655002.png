<script>
var mLatestSemCatID = 457; // XXX: MUST CHANGE! CatID of current year sem
var mLatestSemCatTitle = "2022 Semester 2"; // XXX: MUST CHANGE! Cat name of current year sem
const mCurrentYr = 22; // XXX: MUST CHANGE! Current academic year
const mSupportUsernames = []; // XXX: MUST CHANGE! Names of support users, e.g. const mSupportUsernames = ["Admin User"];

const mOnLoginPage = document.location.href.includes('/login/');
const mOnCourseCatPage = document.location.href.includes('/course/index.php?categoryid=');

var mYrSem = [];
var mCatIDCatNameSorted = [];
var mCourseList = [];

var mIsAdmUser;
var mEnrolledYrSem;
var mEnrolledCourseID;

var mEnhancementDisabled;
var mIsMobile = /iPhone|iPad|iPod|Android/i.test(navigator.userAgent);

var strMyCourses = "";

function mIsAdm() {
    if (mIsAdmUser !== undefined) {
        return mIsAdmUser;
    }
    let username = document.querySelector('span.usertext').innerText;
    let adm = (username.toLowerCase().includes('admin') || (document.querySelectorAll('a.list-group-item[data-key="sitesettings"]').length > 0));
    let support = (username.toLowerCase().includes('support') || mSupportUsernames.includes(username));
    if (adm) {
        document.querySelector('.userbutton').innerHTML = "ADM - " + document.querySelector('.userbutton').innerHTML;
    } else if (support) {
        document.querySelector('.userbutton').innerHTML = "SUP - " + document.querySelector('.userbutton').innerHTML;
    }
    mIsAdmUser = (adm || support);
    return mIsAdmUser;
}

var mFCLR;
var mFCLLL;
var mFCLLink = [];

function mGetFCL() {
    if (mFCLR !== undefined && mFCLLL !== undefined) {
        return;
    }
    if (document.querySelectorAll('.block-fcl__rubric').length > 0 && document.querySelectorAll('a.block-fcl__list__link').length > 0) {
        mFCLR = $('.block-fcl__rubric');
        mFCLLL = $('a.block-fcl__list__link');
        for (let m = 0; m < mFCLR.length; m++) {
            mFCLLink[m] = $('#' + mFCLR[m].getAttribute('aria-controls')).find('a.block-fcl__list__link');
        }
    } else {
        console.log('FCL does not exist. Loading from HOME.');
        $.ajax({
            url: "/index.php",
            type: 'GET',
            async: false, 
            dataType: 'html',
            success: function (data) {
                data = new DOMParser().parseFromString(data, "text/html");
                mFCLR = $(data).find('.block-fcl__rubric');
                mFCLLL = $(data).find('a.block-fcl__list__link');
                for (let m = 0; m < mFCLR.length; m++) {
                    if (!mFCLR[m].innerText.includes('Semester')) {
                        mFCLLink[m] = $(data).find('#' + mFCLR[m].getAttribute('aria-controls')).find('a.block-fcl__list__link');
                    }
                }
            }
        });
    }
}

function mGetEnrolledYrSem() {
    if (mEnrolledYrSem !== undefined && Array.isArray(mEnrolledYrSem)) {
        return mEnrolledYrSem;
    }
    mGetFCL();
    mEnrolledYrSem = [];
    for (let i = 0; i < mFCLR.length; i++) {
        let t = $(mFCLR[i]).text().trim();
        if (t.includes(' Semester ')) {
            mEnrolledYrSem.push(t.split('-')[0] + t.charAt(t.length - 1));
        }
    }
    return mEnrolledYrSem;
}

function mIsEnrolled(courseid) {
    if (mEnrolledCourseID !== undefined && Array.isArray(mEnrolledCourseID)) {
        return mEnrolledCourseID.includes(courseid);
    }
    mGetFCL();
    mEnrolledCourseID = [];
    for (let i = 0; i < mFCLLL.length; i++) {
        mEnrolledCourseID.push($(mFCLLL[i]).attr('href').split("/view.php?id=")[1]);
    }
    mEnrolledCourseID = [...new Set(mEnrolledCourseID)];
    return mEnrolledCourseID.includes(courseid);
}

var mTeacherList = [];

function mGetTeacher(courseid) {
    if (mTeacherList[courseid] != undefined) {
        return mTeacherList[courseid];
    }
    $.ajax({
        type: "GET",
        url: "/course/info.php?id=" + courseid,
        async: false,
        dataType: 'html',
        success: function(data, status) {
            var t = [];
            data = $($.parseHTML(data)).find('ul.teachers').find('a').each(function() {
                t.push($(this).text());
            });
            mTeacherList[courseid] = t.join(', ');   
        }
    });
    return mTeacherList[courseid];
}

function mArrayHasDuplicates(arr) {
    return arr.length !== new Set(arr).size;
}

function mDisableEnhancement() {
    if (mEnhancementDisabled !== undefined) {
        return mEnhancementDisabled;
    }
    let params = new URLSearchParams(document.location.search);
    mEnhancementDisabled = params.has('nofrills');
    return mEnhancementDisabled;
}

document.querySelector("body").style.display = "none"; 
window.addEventListener('load', function() {
    document.querySelector("body").style.display = "block";
});

var mExpandedNav = [];
if (Cookies.get('mExpandedNav') != undefined) {
    mExpandedNav = Cookies.get('mExpandedNav').split(',');
}

for (let i = 0; i < 7; i++) {
    for (let j = 1; j < 4; j++) {
        mYrSem[(mCurrentYr - i).toString() + j] = "20" + (mCurrentYr - i) + " Semester " + j;
    }
}

if (mOnLoginPage || Cookies.get('mCatIDCatNameSorted') == undefined) {
    Cookies.set('mCatIDCatNameSorted', ''); // When mCatIDCatNameSorted is not available in cookies, or sign in Moodle (again), reset them in cookies
}

if (!mOnLoginPage) { 
    // Check if category IDs - mCatIDCatNameSorted available in cookies
    if (Cookies.get('mCatIDCatNameSorted') != '[]' && Cookies.get('mCatIDCatNameSorted') != '') {
        try {
            mCatIDCatNameSorted = JSON.parse(Cookies.get('mCatIDCatNameSorted'));
            console.log('Categories get from cookies.');
        } catch (e) {
            console.log(e);
            mGetCats();
        }
    } else {
        mGetCats();
    }
}

function mFixWidth() {
    if (document.location.href.includes('/course/index.php') || document.location.href.includes('/course/search.php')) {
        try {
            let cardBodyWidth = document.querySelector('.card-body').clientWidth + 'px';
            document.getElementById('region-main-settings-menu').style.width = cardBodyWidth;
            document.getElementById('region-main').style.width = cardBodyWidth;
        } catch (e) {
            console.log(e);
        } 
    }   
}

function mGetCats() {
    // Get category IDs - mCatIDCatNameURLSorted
    $.get("/course/index.php?perpage=all", function(data, status) {
        data = new DOMParser().parseFromString(data, "text/html");
        var mCatIDCatName = [];
        var all = $(data).find('div.info').find('a');
        for (let i = 0; i < all.length; i++) {
            if (all[i].href.includes('?categoryid=')) {
                let catid = all[i].href.split('=')[1];
                let catname = all[i].innerText;
                mCatIDCatName.push(catid + "|" + catname);
            }
        }
        for (let i = 0; i < 7; i++) {
            for (let j = 1; j < 4; j++) {
                for (let m = 0; m < mCatIDCatName.length; m++) {
                    let catname = mCatIDCatName[m].split("|")[1];
                    if (catname.substring(0, 4) == (2001 + mCurrentYr - i).toString() && catname.includes('Semester ' + j)) {
                        mCatIDCatNameSorted.push(mCatIDCatName[m]);
                    }
                }
            }
        }
        // Store the variables to cookies
        Cookies.set('mCatIDCatNameSorted', JSON.stringify(mCatIDCatNameSorted));
