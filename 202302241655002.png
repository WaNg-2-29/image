            let titleHtml = "<table class='mTitle'><tr><td width='100%' style='padding: 10px; font-weight: 700; font-size: 16px'>" + document.querySelector('div.page-header-headings').innerText + "</td></tr></table>";
            let simplesearchform = document.querySelector("div.simplesearchform").insertAdjacentHTML('afterend', titleHtml);
            if (!mIsAdm()) {
                mPrepareCourseList();
            }
        }
        if (document.location.href.endsWith('hkbu.edu.hk') || document.location.href.endsWith('hkbu.edu.hk/') || document.location.href.endsWith('hkbu.edu.hk/index.php')) {
            // document.querySelector('div#page-content').style.display = "none";
            try { 
                if (!mIsAdm()) {
                    let enrolledYrSem = mGetEnrolledYrSem();
                    let latestYrSem = mLatestSemCatTitle.substring(0,4) + mLatestSemCatTitle.charAt(mLatestSemCatTitle.length-1);
                    if (enrolledYrSem.includes(latestYrSem)) {
                        mPrepareCourseList();
                    }
                }
            } catch (e) {
                console.log(e);
            }
        }
        // Search box
        try {
            var searchPlaceholder = '';
            var formControl = document.querySelectorAll('input.form-control');
            for (let i = 0; i < formControl.length; i++) {
                if (formControl[i].id != undefined && formControl[i].id.startsWith('searchinput')) {
                    if (searchPlaceholder == '') {
                        searchPlaceholder = formControl[i].getAttribute('placeholder');
                    }
                    formControl[i].setAttribute('placeholder', '');
                    formControl[i].classList.add('mSearchCourses');
                }
            }    
            var simplesearchform = document.querySelectorAll('div.simplesearchform');
            for (let i = 0; i < simplesearchform.length; i++) {
                let inputgroup = simplesearchform[i].querySelector('div.input-group');
                if (inputgroup != null) {
                    inputgroup.insertAdjacentHTML('beforebegin', '<span class="searchPlaceholder">' + searchPlaceholder + '</span>&nbsp;&nbsp;&nbsp;&nbsp; ');
                }
                if (i > 0) {
                    simplesearchform[i].style.display = "none";
                }
            }
            var searchIcon = document.querySelectorAll('button.search-icon');
            for (let i = 0; i < searchIcon.length; i++) {
                searchIcon[i].classList.remove('btn-primary');
                searchIcon[i].innerHTML = '<img style="width: 16px; height: 16px;" src="data:image/png;base64,">';
            }
        } catch (e) {
            console.log(e);
        }
        // D-flex
        try {
            var dflex = document.querySelector("header#page-header").querySelectorAll(".d-flex");
            dflex[dflex.length-1].style.alignItems = 'center';
            dflex[dflex.length-1].style.justifyContent = 'center';
            dflex[dflex.length-1].style.width = '100%';
        } catch (e) {
            console.log(e);
        }
        // Min-height of #region-main
        try {
            document.getElementById("region-main").style.minHeight = "450px";
        } catch (e) {
            console.log(e);
        }
    });
    window.addEventListener('load', function(event) {
        // mFixWidth();
        setTimeout(mPageReady, 2000);
        if (!mIsAdm()) {
            if (document.location.href.endsWith('/course/index.php')) {
                window.location.replace(window.location.origin);
            }
        }
    });
} else {
    document.addEventListener("DOMContentLoaded", function(event) {
        let v = document.querySelectorAll('div.list-group-item[data-key="mycourses"]');
        for (let i = 0; i < v.length; i++) {
            v[i].style.display = 'block';
        }
        v = document.querySelectorAll('a.list-group-item[data-parent-key="mycourses"]');
        for (let i = 0; i < v.length; i++) {
            v[i].style.display = 'block';
        }
        $('a.list-group-item[data-key="home"]').find('span.media-body').html("<font color='#014898'><b>" + $('a.list-group-item[data-key="home"]').find('span.media-body').text() + "</b></font>");
    });
}
