// New function to analyze course id and name -> id, name, type, tag and link
function mCourseToString(courseid, coursename) {
    var name = "";
    var type = "Others";
    if (coursename.includes('(Section ')) {
        let sections = coursename.substring(coursename.lastIndexOf('(Section ') + 9, coursename.lastIndexOf(')'));
        let sectioncode = [];
        if (sections.includes('/')) {
            sectioncode = sections.split('/');
        } else {
            sectioncode.push(sections);
        }
        let min = Math.min(...sectioncode);
        let max = Math.max(...sectioncode);
        if (max < 10000) {
            type = 'Section ' + sections;
        } else if (min > 9999) {
            type = 'Tutorial ' + sections;
        } else { // max > 9999, and min < 10000
            type = 'Combined ' + sections;
        }
        name = coursename.split(' (Section ')[0];
    } else if (coursename.includes('(All Sections)')) {
        name = coursename.split(' (All Sections)')[0];
        type = 'All Sections';
    } else {
        name = coursename;
    }
    if (coursename.includes('[') && coursename.includes(']') && !name.includes('[') && !name.includes(']')) {
        name = name + ' ' + coursename.substring(coursename.lastIndexOf('['), coursename.lastIndexOf(']') + 1);
    }
    return name + "|" + courseid + "|" + type;
}

