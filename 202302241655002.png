function mPageReady() {
    setTimeout(mAddCatNav, 1000);
    // 20220919: Fixing dragging questions in quiz
    try {
        if (window.location.href.includes('mod/quiz/')) {
            console.log('Fixing dragging questions in quiz');
            $('div.qtext').find('span').each(function() {
                if ($(this).hasClass('drop') && $(this).hasClass('active')) {
                    $(this).css('width', '184px');
                    $(this).css('height', '26px');
                    $(this).css('line-height', '26px');
                }
            });
            $('div.answercontainer').find('span').each(function() {
                if ($(this).hasClass('draghome') && ($(this).hasClass('dragplaceholder') || $(this).hasClass('unplaced'))) {
                    $(this).css('width', '184px');
                    $(this).css('height', '28px');
                    $(this).css('line-height', '26px');
                }
            });
        }
    } catch (e) {
        console.log(e);
    }    
    if (document.location.href.includes('/course/index.php') || document.location.href.endsWith('hkbu.edu.hk') || document.location.href.endsWith('hkbu.edu.hk/') || document.location.href.endsWith('hkbu.edu.hk/index.php')) {
        setTimeout(mMainFadeIn, 1000);
    }
    if (mIsMobile && (document.location.href.endsWith('hkbu.edu.hk') || document.location.href.endsWith('hkbu.edu.hk/') || document.location.href.endsWith('hkbu.edu.hk/index.php'))) {
        setTimeout(mMobileNav, 3000);
    }
    setTimeout(mPageFinalActions, 3000);
    // if (mDisableEnhancement() || mIsAdm()) {
    //     return;
    // }
}

